{
  "env": {
    "UV_PYTHON": "$PWD/.venv/bin/python",
  },
  "packages": [
    "python@3.13",
    "nodejs@latest",
    "uv@latest",
    "duckdb@latest",
  ],
  "env": {
    "DEVBOX_COREPACK_ENABLED": "1",
  },
  "shell": {
    "init_hook": [
      ". $VENV_DIR/bin/activate",
      "uv pip install  -r requirements.txt",
    ],
  },
}
